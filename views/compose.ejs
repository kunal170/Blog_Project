
<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/" method="POST">
<div class="form-group">
<label for="exampleFormControlInput1">Title</label>
<input type="text"class="form-control" id="exampleFormControlInput1" name="titleContent">
</div>

<div class="form-group">
<label for="exampleFormControlTextArea1">Post</label>
<textarea class="form-control" id="exampleFormControlTextArea1" rows="5" name="bodyContent"></textarea>
</div>

<button type="submit" name="publish" value="<%= writtenContent %>" class="btn btn-primary">Publish</button>

</form>

